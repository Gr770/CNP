reset_reg_laqab_effect = {
    if = {
        limit = { has_character_flag = laqab_nobility_faith }
        remove_list_global_variable = { name = laqab_nobility_faith target = root }
        remove_character_flag = laqab_nobility_faith
    }
    else_if = {
        limit = { has_character_flag = laqab_pleased_world_f }
        remove_list_global_variable = { name = laqab_pleased_world_f target = root }
        remove_character_flag = laqab_pleased_world_f
    }
    else_if = {
        limit = { has_character_flag = laqab_pleased_faith_f }
        remove_list_global_variable = { name = laqab_pleased_faith_f target = root }
        remove_character_flag = laqab_pleased_faith_f
    }
    else_if = {
        limit = { has_character_flag = laqab_purity_faith_f }
        remove_list_global_variable = { name = laqab_purity_faith_f target = root }
        remove_character_flag = laqab_purity_faith_f
    }
    else_if = {
        limit = { has_character_flag = laqab_majesty_faith_f }
        remove_list_global_variable = { name = laqab_majesty_faith_f target = root }
        remove_character_flag = laqab_majesty_faith_f
    }
    else_if = {
        limit = { has_character_flag = laqab_knowledgeable_faith_f }
        remove_list_global_variable = { name = laqab_knowledgeable_faith_f target = root }
        remove_character_flag = laqab_knowledgeable_faith_f
    }
    else_if = {
        limit = { has_character_flag = laqab_beauty2_faith }
        remove_list_global_variable = { name = laqab_beauty2_faith target = root }
        remove_character_flag = laqab_beauty2_faith
    }
    else_if = {
        limit = { has_character_flag = laqab_helper3_faith }
        remove_list_global_variable = { name = laqab_helper3_faith target = root }
        remove_character_flag = laqab_helper3_faith
    }
    else_if = {
        limit = { has_character_flag = laqab_pious_faith }
